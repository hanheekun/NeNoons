<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <Button
        android:id="@+id/button_arrow_back"
        android:layout_width="8dp"
        android:layout_height="14dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="42dp"
        android:background="@drawable/arrow_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_arrow_back_background"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="25dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ex_face_1" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView34"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/combined_shape_line1" />

    <TextView
        android:id="@+id/textView_ex_1_b_guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="박자에 맞춰 눈을 깜빡이세요."
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView35"
        app:layout_constraintEnd_toEndOf="@+id/imageView35"
        app:layout_constraintStart_toStartOf="@+id/imageView35"
        app:layout_constraintTop_toTopOf="@+id/imageView35"
        app:layout_constraintVertical_bias="0.39" />

    <Button
        android:id="@+id/button_ex_1_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:background="@drawable/btn_xs_w"
        android:text="진동"
        app:layout_constraintBottom_toTopOf="@+id/button_ex_1_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_ex_1_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_xs_w"
        android:text="깜박임"
        app:layout_constraintBottom_toTopOf="@+id/button_ex_1_5"
        app:layout_constraintStart_toStartOf="@+id/button_ex_1_3"
        app:layout_constraintTop_toBottomOf="@+id/button_ex_1_3" />

    <Button
        android:id="@+id/button_ex_1_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_xs_w"
        android:text="진행과정"
        app:layout_constraintBottom_toTopOf="@+id/imageView35"
        app:layout_constraintStart_toStartOf="@+id/button_ex_1_4"
        app:layout_constraintTop_toBottomOf="@+id/button_ex_1_4" />

    <Button
        android:id="@+id/button_ex_1_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_xs_w"
        android:text="효과음"
        app:layout_constraintBottom_toTopOf="@+id/button_ex_1_4"
        app:layout_constraintStart_toStartOf="@+id/button_ex_1_2"
        app:layout_constraintTop_toBottomOf="@+id/button_ex_1_2" />

    <Button
        android:id="@+id/button_ex_1_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_xs_w"
        android:text="음성"
        app:layout_constraintBottom_toTopOf="@+id/button_ex_1_3"
        app:layout_constraintStart_toStartOf="@+id/button_ex_1_1"
        app:layout_constraintTop_toBottomOf="@+id/button_ex_1_1" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/button_ex_1_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.827"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_ex_1_4"
        app:srcCompat="@drawable/eye_ex_1_open" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="139dp"
        android:layout_height="44dp"
        android:layout_marginStart="43dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_ex_1_5"
        app:layout_constraintStart_toEndOf="@+id/button_ex_1_5"
        app:layout_constraintTop_toTopOf="@+id/button_ex_1_5" />

</androidx.constraintlayout.widget.ConstraintLayout>